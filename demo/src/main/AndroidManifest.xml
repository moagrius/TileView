<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="tileview.demo" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DemoApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
      <activity android:name=".ScrollViewDemoUniversal" />
      <activity android:name=".ScrollViewDemoVertical" />
      <activity android:name=".ScrollViewDemoHorizontal" />
      <activity android:name=".ScalingScrollViewDemoTextViews" />
      <activity android:name=".ScalingScrollViewDemoTiger" />
      <activity android:name=".TileViewDemoAdvanced" />
      <activity android:name=".TileViewDemoSimple" />
    </application>

</manifest>
